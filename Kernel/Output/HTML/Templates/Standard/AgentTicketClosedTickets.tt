[% RenderBlockStart("Overview") %]
<div class="MainBox ARIARoleMain LayoutFixedSidebar SidebarFirst">
    <h1 class="InvisibleText">[% Translate("Closed tickets") | html %]</h1>
    <div class="Clear"></div>

    <div class="ContentColumn">
        <div class="WidgetSimple">
[% RenderBlockStart("OverviewResult") %]
            <div class="Header">
                <h2>[% Translate("List") | html %]</h2>
                <span class="Pagination">
[% RenderBlockStart("OverviewNavBarPageNavBar") %]
                    [% Data.ResultLong %]
                    [% Data.SiteNavBarLong %]
[% RenderBlockEnd("OverviewNavBarPageNavBar") %]
                </span>
            </div>
            <div id="OverviewBody" class="Overview TicketList">
                <table class="TableSmall" id="ClosedTickets">
                    <thead>
                        <tr>
                            <th class="[% Data.QueueCSS %]" title"[% Data.QueueTitle %]"><a name="OverviewControl" href="[% Env("Baselink") %]Action=[% Env("Action") %];SortBy=Queue;OrderBy=[% Data.QueueOrderBy %]">[% Translate("Queue") | html %]</a></th>
                            <th class="[% Data.TicketNumberCSS %]" title"[% Data.TicketNumberTitle %]"><a name="OverviewControl" href="[% Env("Baselink") %]Action=[% Env("Action") %];SortBy=TicketNumber;OrderBy=[% Data.TicketNumberOrderBy %]">[% Translate("TicketNumber") | html %]</a></th>
                            <th class="[% Data.TitleCSS %]" title"[% Data.TitleTitle %]"><a name="OverviewControl" href="[% Env("Baselink") %]Action=[% Env("Action") %];SortBy=Title;OrderBy=[% Data.TitleOrderBy %]">[% Translate("Title") | html %]</a></th>
                            <th class="[% Data.OwnerCSS %]" title"[% Data.OwnerTitle %]"><a name="OverviewControl" href="[% Env("Baselink") %]Action=[% Env("Action") %];SortBy=Owner;OrderBy=[% Data.OwnerOrderBy %]">[% Translate("Owner") | html %]</a></th>
                            <th class="[% Data.CloseTimeCSS %]" title"[% Data.CloseTimeTitle %]"><a name="OverviewControl" href="[% Env("Baselink") %]Action=[% Env("Action") %];SortBy=CloseTime;OrderBy=[% Data.CloseTimeOrderBy %]">[% Translate("CloseTime") | html %]</a></th>
                        </tr>
                    </thead>
                    <tbody>
[% RenderBlockStart("NoDataFoundMsg") %]
                        <tr>
                            <td colspan="5">
                                [% Translate("No data found.") | html %]
                            </td>
                        </tr>
[% RenderBlockEnd("NoDataFoundMsg") %]
[% RenderBlockStart("OverviewResultRow") %]
                        <tr>
                            <td>
                                [% Translate(Data.Queue) | html %]
                            </td>
                            <td>
                                <a class="AsBlock" href="[% Env("Baselink") %]Action=AgentTicketZoom;TicketID=[% Data.TicketID | uri %]">
                                    [% Data.TicketNumber | html %]
                                </a>
                            </td>
                            <td>[% Data.Title %]</td>
                            <td>[% Data.OwnerFullName %]</td>
                            <td>[% Data.CloseTime | Localize("TimeShort") %]</td>
                        </tr>
[% RenderBlockEnd("OverviewResultRow") %]
                        <tr class="FilterMessage Hidden">
                            <td colspan="5">[% Translate("No matches found.") | html %]</td>
                        </tr>
                    </tbody>
                </table>
            </div>
[% RenderBlockEnd("OverviewResult") %]
        <div class="Clear"></div>
        </div>
    </div>
    <div class="Clear"></div>
</div>
[% RenderBlockEnd("Overview") %]
